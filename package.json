{
  "name": "alphaarena-live",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "tsx scripts/generate-articles-data.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "postinstall": "prisma generate",
    "generate-articles": "tsx scripts/generate-article.ts",
    "generate-ai-articles": "tsx scripts/generate-ai-article.ts",
    "generate-seo-article": "tsx scripts/generate-seo-article.ts",
    "generate-articles-data": "tsx scripts/generate-articles-data.ts",
    "sync-advanced-data": "tsx scripts/sync-advanced-data.ts",
    "pages:build": "prisma generate && next build",
    "pages:dev": "next dev",
    "pages:deploy": "echo 'Please deploy via Cloudflare Dashboard (Git integration)'",
    "workers:build": "prisma generate && tsx scripts/generate-articles-data.ts && next build && npx @opennextjs/cloudflare build",
    "workers:deploy": "pnpm workers:build && wrangler deploy",
    "workers:dev": "wrangler dev",
    "wrangler": "wrangler"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.0",
    "@tanstack/react-query": "^5.90.5",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.546.0",
    "next": "14.2.18",
    "openai": "^6.6.0",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.3.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "schema-dts": "^1.1.5",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@opennextjs/cloudflare": "^1.11.0",
    "@types/cheerio": "^1.0.0",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vercel": "^48.6.0",
    "wrangler": "^4.45.0"
  },
  "packageManager": "pnpm@10.18.3"
}
